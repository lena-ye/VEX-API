{
  "Award": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "format": "int32"
      },
      "event": {
        "_originalRef": "#/components/schemas/IdInfo",
        "type": "object",
        "required": [
          "id",
          "name"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "code": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "order": {
        "type": "integer",
        "format": "int32"
      },
      "title": {
        "type": "string"
      },
      "qualifications": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "designation": {
        "type": "string",
        "enum": [
          "tournament",
          "division"
        ],
        "nullable": true,
        "description": "Some awards are given out per tournament or division"
      },
      "classification": {
        "type": "string",
        "enum": [
          "champion",
          "finalist",
          "semifinalist",
          "quarterfinalist"
        ],
        "nullable": true
      },
      "teamWinners": {
        "type": "array",
        "items": {
          "_originalRef": "#/components/schemas/TeamAwardWinner",
          "type": "object",
          "properties": {
            "division": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "team": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            }
          }
        }
      },
      "individualWinners": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
}
{
  "PaginatedMatch": {
    "type": "object",
    "properties": {
      "meta": {
        "_originalRef": "#/components/schemas/PageMeta",
        "type": "object",
        "properties": {
          "current_page": {
            "type": "integer"
          },
          "first_page_url": {
            "type": "string"
          },
          "from": {
            "type": "integer"
          },
          "last_page": {
            "type": "integer"
          },
          "last_page_url": {
            "type": "string"
          },
          "next_page_url": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "per_page": {
            "type": "integer"
          },
          "prev_page_url": {
            "type": "string"
          },
          "to": {
            "type": "integer"
          },
          "total": {
            "type": "integer"
          }
        }
      },
      "data": {
        "type": "array",
        "items": {
          "_originalRef": "#/components/schemas/MatchObj",
          "type": "object",
          "required": [
            "id",
            "event",
            "division",
            "round",
            "instance",
            "matchnum",
            "scored",
            "alliances",
            "name"
          ],
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "event": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "division": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "round": {
              "type": "integer",
              "format": "int32"
            },
            "instance": {
              "type": "integer",
              "format": "int32"
            },
            "matchnum": {
              "type": "integer",
              "format": "int32"
            },
            "scheduled": {
              "type": "string",
              "format": "date-time"
            },
            "started": {
              "type": "string",
              "format": "date-time"
            },
            "field": {
              "type": "string"
            },
            "scored": {
              "type": "boolean"
            },
            "name": {
              "type": "string"
            },
            "alliances": {
              "type": "array",
              "items": {
                "_originalRef": "#/components/schemas/Alliance",
                "type": "object",
                "required": [
                  "color",
                  "score",
                  "teams"
                ],
                "properties": {
                  "color": {
                    "type": "string",
                    "enum": [
                      "red",
                      "blue"
                    ]
                  },
                  "score": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "teams": {
                    "type": "array",
                    "items": {
                      "_originalRef": "#/components/schemas/AllianceTeam",
                      "type": "object",
                      "properties": {
                        "team": {
                          "_originalRef": "#/components/schemas/IdInfo",
                          "type": "object",
                          "required": [
                            "id",
                            "name"
                          ],
                          "properties": {
                            "id": {
                              "type": "integer",
                              "format": "int32"
                            },
                            "name": {
                              "type": "string"
                            },
                            "code": {
                              "type": "string",
                              "nullable": true
                            }
                          }
                        },
                        "sitting": {
                          "type": "boolean"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
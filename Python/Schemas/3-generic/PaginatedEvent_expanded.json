{
  "PaginatedEvent": {
    "type": "object",
    "properties": {
      "meta": {
        "_originalRef": "#/components/schemas/PageMeta",
        "type": "object",
        "properties": {
          "current_page": {
            "type": "integer"
          },
          "first_page_url": {
            "type": "string"
          },
          "from": {
            "type": "integer"
          },
          "last_page": {
            "type": "integer"
          },
          "last_page_url": {
            "type": "string"
          },
          "next_page_url": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "per_page": {
            "type": "integer"
          },
          "prev_page_url": {
            "type": "string"
          },
          "to": {
            "type": "integer"
          },
          "total": {
            "type": "integer"
          }
        }
      },
      "data": {
        "type": "array",
        "items": {
          "_originalRef": "#/components/schemas/Event",
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "sku": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "start": {
              "type": "string",
              "format": "date-time"
            },
            "end": {
              "type": "string",
              "format": "date-time"
            },
            "season": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "program": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "location": {
              "_originalRef": "#/components/schemas/Location",
              "type": "object",
              "properties": {
                "venue": {
                  "type": "string"
                },
                "address_1": {
                  "type": "string"
                },
                "address_2": {
                  "type": "string"
                },
                "city": {
                  "type": "string"
                },
                "region": {
                  "type": "string"
                },
                "postcode": {
                  "type": "string"
                },
                "country": {
                  "type": "string"
                },
                "coordinates": {
                  "_originalRef": "#/components/schemas/Coordinates",
                  "type": "object",
                  "properties": {
                    "lat": {
                      "type": "number",
                      "format": "float"
                    },
                    "lon": {
                      "type": "number",
                      "format": "float"
                    }
                  }
                }
              }
            },
            "locations": {
              "type": "array",
              "items": {
                "_originalRef": "#/components/schemas/Locations",
                "type": "object",
                "additionalProperties": {
                  "_originalRef": "#/components/schemas/Location",
                  "type": "object",
                  "properties": {
                    "venue": {
                      "type": "string"
                    },
                    "address_1": {
                      "type": "string"
                    },
                    "address_2": {
                      "type": "string"
                    },
                    "city": {
                      "type": "string"
                    },
                    "region": {
                      "type": "string"
                    },
                    "postcode": {
                      "type": "string"
                    },
                    "country": {
                      "type": "string"
                    },
                    "coordinates": {
                      "_originalRef": "#/components/schemas/Coordinates",
                      "type": "object",
                      "properties": {
                        "lat": {
                          "type": "number",
                          "format": "float"
                        },
                        "lon": {
                          "type": "number",
                          "format": "float"
                        }
                      }
                    }
                  }
                }
              }
            },
            "divisions": {
              "type": "array",
              "items": {
                "_originalRef": "#/components/schemas/Division",
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "name": {
                    "type": "string"
                  },
                  "order": {
                    "type": "integer",
                    "format": "int32"
                  }
                }
              }
            },
            "level": {
              "_originalRef": "#/components/schemas/EventLevel",
              "type": "string",
              "enum": [
                "World",
                "National",
                "Regional",
                "State",
                "Signature",
                "Other"
              ]
            },
            "ongoing": {
              "type": "boolean"
            },
            "awards_finalized": {
              "type": "boolean"
            },
            "event_type": {
              "_originalRef": "#/components/schemas/EventType",
              "type": "string",
              "enum": [
                "tournament",
                "league",
                "workshop",
                "virtual"
              ]
            }
          },
          "required": [
            "id",
            "sku",
            "name",
            "season",
            "program",
            "location"
          ]
        }
      }
    }
  }
}
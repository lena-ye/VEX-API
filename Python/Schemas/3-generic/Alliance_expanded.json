{
  "Alliance": {
    "type": "object",
    "required": [
      "color",
      "score",
      "teams"
    ],
    "properties": {
      "color": {
        "type": "string",
        "enum": [
          "red",
          "blue"
        ]
      },
      "score": {
        "type": "integer",
        "format": "int32"
      },
      "teams": {
        "type": "array",
        "items": {
          "_originalRef": "#/components/schemas/AllianceTeam",
          "type": "object",
          "properties": {
            "team": {
              "_originalRef": "#/components/schemas/IdInfo",
              "type": "object",
              "required": [
                "id",
                "name"
              ],
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "nullable": true
                }
              }
            },
            "sitting": {
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}